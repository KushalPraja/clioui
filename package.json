{
  "name": "@yourscope/mojoui",
  "private": false,
  "version": "0.1.0",
  "description": "A React UI component library built with Radix UI and Tailwind",
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/mojoui.git"
  },
  "keywords": [
    "react",
    "ui",
    "components",
    "radix-ui",
    "tailwind"
  ],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "mojoui": "./dist/cli/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run clean && npm run build:lib && npm run build:cli",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint src/**/*.ts*",
    "clean": "rimraf dist",
    "build:lib": "tsc -p tsconfig.json",
    "build:cli": "tsc -p tsconfig.cli.json && shx chmod +x ./dist/cli/index.js",
    "prepublishOnly": "npm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-tooltip": "^1.1.8",
    "clsx": "^2.0.0",
    "commander": "^11.0.0",
    "prompts": "^2.4.2",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.0.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "tailwindcss": "^3.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@eslint/js": "^9.19.0",
    "@storybook/addon-essentials": "^8.5.6",
    "@storybook/addon-interactions": "^8.5.6",
    "@storybook/addon-onboarding": "^8.5.6",
    "@storybook/blocks": "^8.5.6",
    "@storybook/react": "^8.5.6",
    "@storybook/react-vite": "^8.5.6",
    "@storybook/test": "^8.5.6",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "eslint-plugin-storybook": "^0.11.3",
    "globals": "^15.14.0",
    "postcss": "^8.4.35",
    "shx": "^0.3.4",
    "storybook": "^8.5.6",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
